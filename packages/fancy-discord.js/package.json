{
    "name": "fancy-discord.js",
    "version": "0.7.0",
    "description": "discord.js, but fancier.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlfieGoldson/fancy-discord.git"
    },
    "keywords": [
        "Discord",
        "Router",
        "Commands"
    ],
    "author": {
        "name": "AlfieGoldson"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "dev": "ts-node-dev src/index.ts",
        "lint": "eslint '*/**/*.{ts,tsx}' --fix --ignore-path .eslintignore",
        "typecheck": "tsc --noemit",
        "test": "jest --coverage --verbose",
        "test:watch": "jest --watch",
        "prepare": "yarn build",
        "prepublishOnly": "yarn test && yarn lint",
        "preversion": "yarn test && yarn lint",
        "version": "yarn lint && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "ts-jest": "^26.5.5",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "discord.js": "^12.5.3"
    },
    "bugs": {
        "url": "https://github.com/AlfieGoldson/fancy-discord/issues"
    },
    "homepage": "https://github.com/AlfieGoldson/fancy-discord#readme",
    "files": [
        "lib/**/*",
        "embeds.js",
        "embeds.d.ts"
    ]
}
